          <drools:metaValue><![CDATA[Publish Remaining Budget]]></drools:metaValue>
        </drools:metaData>
        <drools:onEntry-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[kcontext.setVariable("topic","budget");

budget = org.pamenon.jbpm.claimfund.util.Budget.getInstance();

budget.total = budget.total -  loan.getAmount(); 

System.out.println("Remaining Budget: "+budget.total);

String json = "{ \"budget\": " 
				+ loan.getAmount()+ 
				 "}";

kcontext.setVariable("payload",json);

System.out.println(" Budget: " +kcontext.getVariable("payload"));]]></drools:script>
        </drools:onEntry-script>
